	<queryString language="SQL">
		<![CDATA[SELECT distinct
inv.activos.cod_ant,
inv.activos.can,
inv.activos.uni_med,
inv.activos.des,
inv.activos.des_det ,
inv.activos.imp_bs ,
(select count(*)from inv.activos ac where ac.nro_doc = inv.activos.nro_doc)*inv.activos.imp_bs as total,
(select count(*)from inv.activos ac where ac.nro_doc = inv.activos.nro_doc) as cantidad,
inv.activos.estado,
inv.activos.nro_doc ,
inv.sub_oficinas.descripcion as sub_ofc_des,
inv.oficinas.descripcion as ofc_des,
inv.activos.ofc_cod , 
inv.activos.sub_ofc_cod , 
inv.activos.par_cod,
 act.partida.par_des 
FROM inv.activos, personas , inv.cargos , inv.sub_oficinas,  inv.oficinas , act.partida 
WHERE 
CAST(inv.activos.sub_ofc_cod AS text) IN (select unnest(string_to_array( $P{cargo} , ',')) as subUnidad)
and personas.nro_dip = inv.activos.ci_resp
and  inv.cargos.id = inv.activos.car_cod
and  inv.activos.ofc_cod like $P{unidad} 
and  inv.sub_oficinas.id = inv.activos.sub_ofc_cod 
and  inv.activos.ofc_cod =  inv.oficinas.cod_soa 
and  act.partida.par_cod =  inv.activos.par_cod 
and  act.partida.gestion = 2020
ORDER BY inv.activos.ofc_cod , inv.activos.sub_ofc_cod , inv.activos.par_cod, inv.activos.nro_doc]]>
	</queryString>
