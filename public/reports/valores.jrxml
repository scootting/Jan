	<queryString>
		<![CDATA[SELECT MONTH(vt.fec_tra) as mes, round(sum(vt.imp_val)/min(vt.pre_uni),2) as cantidad, min(vt.pre_uni) as precio, sum(vt.imp_val)  as importe FROM val.tra_dia vt
	    where vt.cod_val = '8250'
          and vt.gestion = '2017'
          and vt.tip_tra = '1'          
     group by MONTH(vt.fec_tra)
     order by MONTH(vt.fec_tra) asc]]>
	</queryString>
