	<parameter name="p_unidad" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT b.nro_doc, 
                       b.par_cod,
                       b.des, b.des_det, 
                       b.uni_med, 
                       b.can, 
                       b.imp_bs, 
                       b.estado, 
                       b.ofc_cod,
                       (select d.descripcion from inv.oficinas d
where d.cod_soa = $P{p_unidad}  ) as oficina,
                       b.sub_ofc_cod, 
                       b.car_cod, 
                       b.ci_resp::VARCHAR(10),
                       (select p.par_des from act.partida p
where p.par_cod = b.par_cod and p.gestion = '2021') AS par_des  
   				  FROM ((SELECT a.ofc_cod, a.codigo AS nro_doc, a.act_par_cod AS par_cod,
                       			a.act_can AS can, 'PIEZA' AS uni_med, a.act_des AS des, a.act_des_det AS des_det, 
                       			a.act_imp_bs AS imp_bs, a.act_estado AS estado, a.sub_ofc_cod, a.car_cod,
                       			a.act_ci_resp AS ci_resp
                  		   FROM act.activos a)
                       UNION ALL
                       (SELECT i.ofc_cod, (i.nro_doc ||'.'|| i.idc) as nro_doc, i.par_cod,
                                i.can, i.uni_med, i.des, i.des_det,
                                i.imp_bs, i.estado, i.sub_ofc_cod, i.car_cod,
                                i.ci_resp
                       	   FROM inv.activos i)
                       ) AS b
                 WHERE b.ofc_cod =  $P{p_unidad} 
                 ORDER BY b.par_cod]]>
	</queryString>
