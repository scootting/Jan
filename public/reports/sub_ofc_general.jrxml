	<parameter name="p_sub_unidad" class="java.lang.String"/>
	<parameter name="p_unidad" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT *,
       (SELECT p.par_des 
          FROM act.partida p
         WHERE p.par_cod = a.par_cod AND p.gestion = '2021') AS par_des,                       
       (SELECT d.descripcion 
          FROM inv.oficinas d
         WHERE d.cod_soa = $P{p_unidad}) AS ofc_des,
       (SELECT s.descripcion 
          FROM inv.sub_oficinas s
         WHERE s.id = sub_ofc_cod) AS sub_ofc_des        
  FROM act.vv_act_general a
 WHERE a.ofc_cod = $P{p_unidad}
 AND CAST(sub_ofc_cod AS text) IN (SELECT unnest(string_to_array($P{p_sub_unidad}  , ',')) AS sub_ofc_cod)
 ORDER BY a.par_cod ASC
]]>
	</queryString>
