	<queryString>
		<![CDATA[select d.obs_est,d.validacion,a.act_des,a.act_des_det,a.nro_doc,a.par_cod
,a.ofc_cod,a.sub_ofc_cod,a.act_imp , a.act_can ,a.act_uni_med,
(SELECT o.descripcion 
          FROM inv.oficinas o
         WHERE o.cod_soa =  $P{p_unidad}  limit 1) AS ofc_des,
(SELECT s.descripcion 
          FROM inv.sub_oficinas s
         WHERE s.id = sub_ofc_cod) AS sub_ofc_des, 
(SELECT p.par_des 
FROM act.partida p 
WHERE p.par_cod = a.par_cod and p.gestion = 2021) as par_des,
(SELECT e.desc from inv.estado e
	where e.id = d.est_act
 )as estado
from inv.detalle_doc_act as d
INNER JOIN act.vv_act_detallado as a
ON d.id_act = a.id
AND d.nro_doc_inv like  $P{p_no_doc} 
AND a.ofc_cod like   $P{p_unidad} 
AND d.validacion = false
AND CAST(a.sub_ofc_cod AS text) IN (SELECT unnest(string_to_array( '1' , ',')) AS sub_ofc_cod)
order by a.sub_ofc_cod, a.par_cod]]>
	</queryString>
