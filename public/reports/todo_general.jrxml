	<parameter name="unidad" class="java.lang.String"/>
	<queryString>
		<![CDATA[select 
b.nro_doc::VARCHAR(20), 
   					   b.par_cod,
(select p.par_des from act.partida p
where p.par_cod = b.par_cod and p.gestion = '2021') as partida,                       
                       b.des, 
                       b.des_det, 
                       b.uni_med, 
                       sum(b.can), 
                       sum(b.imp_bs) as total, 
                       b.ofc_cod,
(select d.descripcion from inv.oficinas d
where d.cod_soa = $P{unidad}  ) as oficina,
                       b.sub_ofc_cod, 
                       b.car_cod, 
                       b.ci_resp::VARCHAR(10) 
   				  FROM ((SELECT a.ofc_cod, iif(POSITION('.' IN a.codigo)<>0,substr(a.codigo,0,POSITION('.' IN a.codigo)),a.codigo ) AS nro_doc,
                                a.act_par_cod AS par_cod, a.act_can AS can, 'PIEZA' AS uni_med, a.act_des AS des,
                                a.act_des_det AS des_det, a.act_imp_bs AS imp_bs, a.sub_ofc_cod, a.car_cod,
                                a.act_ci_resp AS ci_resp
                           FROM act.activos a)
                       UNION ALL
                        (SELECT i.ofc_cod, i.nro_doc,
                                i.par_cod, i.can, i.uni_med, i.des,
                                i.des_det, i.imp_bs, i.sub_ofc_cod, i.car_cod,
                                i.ci_resp
                           FROM inv.activos i)
                       ) AS b
   			     WHERE b.ofc_cod =  $P{unidad} 
              GROUP BY b.nro_doc, b.par_cod, b.des, b.des_det, b.uni_med, b.ofc_cod, b.sub_ofc_cod, b.car_cod, b.ci_resp
              ORDER BY b.par_cod asc]]>
	</queryString>
