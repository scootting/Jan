	<parameter name="subUnidad" class="java.lang.String"/>
	<parameter name="unidad" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT personas.paterno , personas.materno , personas.nombres ,
 inv.cargos.descripcion,  inv.activos.cod_ant,
	inv.activos.can,
	inv.activos.uni_med,
	inv.activos.des,
	inv.activos.estado,
	inv.activos.nro_doc ,
	inv.sub_oficinas.descripcion 
FROM inv.activos, personas , inv.cargos , inv.sub_oficinas 
WHERE CAST(inv.activos.sub_ofc_cod AS text) IN (select unnest(string_to_array( $P{subUnidad} , ',')) as car_cod)
and personas.nro_dip = inv.activos.ci_resp
 and  inv.cargos.id = inv.activos.car_cod
 and  inv.activos.ofc_cod like $P{unidad} 
 and  inv.sub_oficinas.id = inv.activos.sub_ofc_cod 
GROUP BY personas.paterno , personas.materno , 
personas.nombres ,inv.cargos.descripcion,
inv.activos.cod_ant,
	inv.activos.can,
	inv.activos.uni_med,
	inv.activos.des,
	inv.activos.estado,
	inv.activos.nro_doc,
	 inv.sub_oficinas.descripcion 
ORDER BY inv.activos.nro_doc DESC]]>
	</queryString>
