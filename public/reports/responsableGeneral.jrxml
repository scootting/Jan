	<parameter name="ci_list" class="java.lang.String"/>
	<parameter name="unidad" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT distinct
per.paterno , 
per.materno , 
per.nombres ,
c.descripcion as cargo,  
ac.cod_ant,
ac.can,
ac.estado,
ac.nro_doc,
ac.can_t,
ac.uni_med,
ac.des,
ac.des_det ,
of.descripcion as oficina, 
ac.can_t*ac.imp_bs as total,
ac.ofc_cod, 
ac.sub_ofc_cod,
ac.par_cod,
sof.descripcion as sub_ofc_des,
p.par_des 
FROM 
inv.activos ac
INNER JOIN inv.oficinas of ON ac.ofc_cod = of.cod_soa
INNER JOIN inv.sub_oficinas sof ON ac.sub_ofc_cod = sof.id 
INNER JOIN act.partida p ON ac.par_cod = p.par_cod
INNER JOIN inv.cargos c ON ac.car_cod = c.id 
INNER JOIN personas per ON ac.ci_resp = per.nro_dip 
and CAST(ac.ci_resp AS text) IN (select unnest(string_to_array( $P{ci_list}  , ',')) as ci_resp)
and ac.ofc_cod like $P{unidad} 
and p.gestion = 2020
ORDER BY ac.ofc_cod,ac.sub_ofc_cod,ac.par_cod,ac.nro_doc


]]>
	</queryString>
