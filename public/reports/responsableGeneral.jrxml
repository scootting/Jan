	<parameter name="ci_list" class="java.lang.String"/>
	<parameter name="unidad" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT distinct
personas.paterno , 
personas.materno , 
personas.nombres ,
inv.cargos.descripcion,  
inv.activos.cod_ant,
inv.activos.can,
inv.activos.uni_med,
inv.activos.des,
inv.activos.des_det ,
inv.activos.estado,
inv.activos.nro_doc,
inv.activos.par_cod,
 act.partida.par_des,
 inv.activos.ofc_cod,
(select count(*)from inv.activos ac where ac.nro_doc = inv.activos.nro_doc)*inv.activos.imp_bs as total
FROM inv.activos, personas , inv.cargos ,  inv.oficinas , act.partida 
WHERE CAST(inv.activos.ci_resp AS text) IN (select unnest(string_to_array( $P{ci_list}  , ',')) as ci_resp)
and personas.nro_dip = inv.activos.ci_resp
and  inv.cargos.id = inv.activos.car_cod
and  inv.activos.ofc_cod like $P{unidad} 
and  inv.activos.ofc_cod =  inv.oficinas.cod_soa 
and  act.partida.par_cod =  inv.activos.par_cod 
and  act.partida.gestion = 2020
ORDER BY  inv.activos.ofc_cod , inv.activos.par_cod, inv.activos.nro_doc

]]>
	</queryString>
