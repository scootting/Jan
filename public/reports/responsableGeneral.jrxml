	<parameter name="ci_list" class="java.lang.String"/>
	<parameter name="unidad" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT b.nro_doc::VARCHAR(20), 
   					   b.par_cod,
                       b.des, 
                       b.des_det, 
                       b.uni_med, 
                       sum(b.can), 
                       sum(b.imp_bs) as total, 
                       b.ofc_cod,
                       b.sub_ofc_cod, 
                       b.car_cod, 
                       b.ci_resp::VARCHAR(10) ,
                       o.descripcion as oficina,
					p.par_des as partida,
					per.paterno,per.materno,per.nombres ,c.descripcion as cargo
   				  FROM ((SELECT a.ofc_cod, iif(POSITION('.' IN a.codigo)<>0,substr(a.codigo,0,POSITION('.' IN a.codigo)),a.codigo ) AS nro_doc,
                                a.act_par_cod AS par_cod, a.act_can AS can, 'PIEZA' AS uni_med, a.act_des AS des,
                                a.act_des_det AS des_det, a.act_imp_bs AS imp_bs, a.sub_ofc_cod, a.car_cod,
                                a.act_ci_resp AS ci_resp
                           FROM act.activos a)
                       UNION ALL
                        (SELECT i.ofc_cod, i.nro_doc,
                                i.par_cod, i.can, i.uni_med, i.des,
                                i.des_det, i.imp_bs, i.sub_ofc_cod, i.car_cod,
                                i.ci_resp
                           FROM inv.activos i)
                       ) AS b , inv.oficinas o , act.partida p,personas per ,inv.cargos c
   			     WHERE b.ofc_cod = $P{unidad} 
   			     and CAST(b.ci_resp AS text) IN (select unnest(string_to_array( $P{ci_list} , ',')) as ci_resp)
   			     and b.ofc_cod = o.cod_soa and b.par_cod = p.par_cod and b.ci_resp = per.nro_dip and b.car_cod = c.id
              GROUP BY b.nro_doc, b.par_cod, b.des, b.des_det, b.uni_med, b.ofc_cod, b.sub_ofc_cod, b.car_cod, b.ci_resp, o.descripcion,p.par_des,per.paterno,per.materno,per.nombres,c.descripcion
              ORDER BY b.par_cod asc]]>
	</queryString>
