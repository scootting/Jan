	<queryString language="SQL">
		<![CDATA[SELECT personas.paterno , personas.materno , personas.nombres ,
 inv.cargos.descripcion,  inv.activos.cod_ant,
	inv.activos.can,
	inv.activos.uni_med,
	inv.activos.des,
	inv.activos.estado,
	inv.activos.nro_doc
FROM inv.activos, personas , inv.cargos 
WHERE CAST(inv.activos.ci_resp AS text) IN (select unnest(string_to_array( $P{ci_list}  , ',')) as car_cod)
and personas.nro_dip = inv.activos.ci_resp
 and  inv.cargos.id = inv.activos.car_cod
 and  inv.activos.ofc_cod like $P{unidad} 
GROUP BY personas.paterno , personas.materno , 
personas.nombres ,inv.cargos.descripcion,
inv.activos.cod_ant,
	inv.activos.can,
	inv.activos.uni_med,
	inv.activos.des,
	inv.activos.estado,
	inv.activos.nro_doc
ORDER BY inv.activos.nro_doc DESC]]>
	</queryString>
