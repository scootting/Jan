	<parameter name="unidad" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
 personas.paterno ,
 personas.materno , 
 personas.nombres ,
 inv.cargos.descripcion, 
 inv.activos.cod_ant,
 inv.activos.can,
inv.activos.uni_med,
inv.activos.des,
inv.activos.estado,
inv.activos.nro_doc,
inv.activos.idc ,
inv.activos.imp_bs , 
inv.activos.des_det ,
inv.activos.par_cod , 
act.partida.par_des 
FROM inv.activos, personas , inv.cargos , act.partida, inv.oficinas 
WHERE CAST(inv.activos.ci_resp AS text) IN (select unnest(string_to_array( $P{ci_resp} , ',')) as ci_resp)
and personas.nro_dip = inv.activos.ci_resp
and  inv.cargos.id = inv.activos.car_cod
and  inv.activos.ofc_cod like  $P{unidad} 
and  inv.oficinas.cod_soa = inv.activos.ofc_cod
and inv.activos.par_cod =  act.partida.par_cod
and  act.partida.gestion = 2020
ORDER BY inv.activos.ofc_cod ,inv.activos.ci_resp , inv.activos.par_cod, inv.activos.nro_doc

]]>
	</queryString>
