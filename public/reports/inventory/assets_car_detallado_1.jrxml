	<parameter name="p_unidad" class="java.lang.String"/>
	<parameter name="p_car_unidad" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT *,
       (SELECT p.par_des 
          FROM act.partida p
         WHERE p.par_cod = a.par_cod AND p.gestion = '2021') AS par_des,                       
       (SELECT d.descripcion 
          FROM inv.oficinas d
         WHERE d.cod_soa = $P{p_unidad} limit 1) AS ofc_des,
       (SELECT s.descripcion 
          FROM inv.cargos s
         WHERE s.id = car_cod) AS car_des        
  FROM act.vv_act_detallado a
 WHERE a.ofc_cod = $P{p_unidad}
 AND CAST(car_cod AS text) IN (SELECT unnest(string_to_array($P{p_car_unidad}  , ',')) AS car_cod)
 ORDER BY a.car_cod, a.par_cod ASC]]>
	</queryString>
